{
  "name": "language-pde",
  "displayName": "Processing Language",
  "description": "Processing Language Support for VSCode",
  "version": "0.0.3",
  "publisher": "Tobiah",
  "engines": {
    "vscode": "^0.10.1"
  },
  "license": "MIT",
  "homepage": "https://github.com/TobiahZ/processing-vscode",
  "repository": {
    "type": "git",
    "url": "https://github.com/TobiahZ/processing-vscode.git"
  },
  "categories": [
    "Languages",
    "Snippets"
  ],
  "activationEvents": [
    "onCommand:extension.sayHello"
  ],
  "main": "./out/src/extension",
  "contributes": {
		"commands": [{
			"command": "extension.sayHello",
			"title": "Hello World"
		}],
    "languages": [
      {
        "id": "pde",
        "aliases": [
          "Processing",
          "pde"
        ],
        "extensions": [
          ".pde",
          ".java"
        ],
        "configuration": "./pde.configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "pde",
        "scopeName": "source.pde",
        "path": "./syntaxes/pde.tmLanguage"
      }
    ],
    "snippets": [
      {
        "language": "pde",
        "path": "./snippets/snippets.json"
      }
    ]
  },
	"scripts": {
		"vscode:prepublish": "node ./node_modules/vscode/bin/compile",
		"compile": "node ./node_modules/vscode/bin/compile -watch -p ./",
        "pretest": "tsc",
        "test": "mocha --ui tdd ./out/test/extension.test.js"
	},
  "devDependencies": {
    "typescript": "^1.6.2",
    "vscode": "^0.10.x"
  },
  "icon": "processing.png"
}
